{
  "manifest_version": 2,
  "name": "Move Tab to Next Window",
  "version": "0.9.1",
  "description": "Move tab to next window - tab context menu and button extension",
  "homepage_url": "https://github.com/rshev/MoveTabToNextWindow",
  "permissions": ["activeTab", "contextMenus"],
  "background": {
    "scripts": ["index.js"]
  },
  "icons": {
    "48": "icon-dark-outlined-48.png",
    "96": "icon-dark-outlined-96.png"
  },
  "browser_action": {
    "default_title": "Move tab to the next window",
    "default_icon": "icon-dark.svg",
    "theme_icons": [
      {
        "dark": "icon-dark.svg",
        "light": "icon-light.svg",
        "size": 16
      },
      {
        "dark": "icon-dark.svg",
        "light": "icon-light.svg",
        "size": 32
      }
    ]
  }
}
