{
  "manifest_version": 2,
  "name": "Move Tab to Next Window",
  "version": "0.9.0",
  "description": "Move tab to next window - tab context menu extension",
  "homepage_url": "https://github.com/rshev/MoveTabToNextWindow",
  "permissions": ["activeTab", "contextMenus"],
  "background": {
    "scripts": ["index.js"]
  },
  "icons": {
    "16": "icon-dark-16.png",
    "32": "icon-dark-32.png",
    "48": "icon-dark-48.png",
    "96": "icon-dark-96.png",
    "128": "icon-dark-128.png"
  },
  "browser_action": {
    "default_title": "Move tab to the next window",
    "default_icon": {
      "16": "icon-light-outlined-16.png",
      "32": "icon-light-outlined-32.png"
    },
    "theme_icons": [
      {
        "dark": "icon-dark.svg",
        "light": "icon-light.svg",
        "size": 16
      },
      {
        "dark": "icon-dark.svg",
        "light": "icon-light.svg",
        "size": 32
      }
    ]
  }
}
